{
  "name": "ember-cli-jss",
  "version": "1.0.1",
  "description": "JSS integration for Ember",
  "license": "MIT",
  "author": "Timofey Dergachev <t@exeto.me> (https://exeto.me/)",
  "repository": "exeto/ember-cli-jss",
  "scripts": {
    "start": "ember test --server",
    "test": "ember try:each",
    "precommit": "lint-staged"
  },
  "files": [
    "addon",
    "app",
    "config/environment.js",
    "index.js",
    "lib",
    "vendor"
  ],
  "keywords": [
    "ember-addon",
    "jss",
    "style",
    "sheet",
    "stylesheet",
    "css",
    "components",
    "composable",
    "css in js",
    "css-in-js"
  ],
  "engines": {
    "node": ">= 8.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --fix",
      "git add"
    ],
    "**/*.{json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "ember-cli-babel": "7.1.2"
  },
  "devDependencies": {
    "@exeto/eslint-config": "^2.0.0",
    "ember-cli": "~3.4.0",
    "ember-cli-dependency-checker": "^3.0.0",
    "ember-cli-htmlbars": "^3.0.0",
    "ember-cli-htmlbars-inline-precompile": "^1.0.0",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-qunit": "^4.1.1",
    "ember-disable-prototype-extensions": "^1.1.2",
    "ember-load-initializers": "^1.0.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-resolver": "^5.0.0",
    "ember-source": "~3.4.0",
    "ember-source-channel-url": "^1.0.1",
    "ember-try": "^1.0.0-beta.2",
    "eslint": "^5.0.0",
    "eslint-config-prettier": "^3.0.0",
    "eslint-plugin-ember": "5.2.0",
    "eslint-plugin-prettier": "^3.0.0",
    "husky": "^1.1.0",
    "lint-staged": "^7.0.0",
    "loader.js": "^4.2.3",
    "prettier": "^1.14.2"
  }
}
